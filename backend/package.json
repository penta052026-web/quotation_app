{
  "name": "quotation-app-backend",
  "version": "1.0.0",
  "description": "Backend for Quotation Generation Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:excel": "node scripts/migrate-excel-to-sql.js",
    "db:update": "node scripts/update-database.js",
    "db:reset": "npx knex migrate:rollback --all && npx knex migrate:latest"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.31",
    "knex": "^3.0.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^24.34.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "quotation",
    "excel",
    "pdf",
    "generator"
  ],
  "author": "Quotation App",
  "license": "MIT"
}
